{
	"name": "Feedly Window Tab",
	"version": "0.31",
	"manifest_version": 3,
	"description": "Open Feedly Links in a Target Window's Tab using shortcut key",
	"content_scripts": [
		{
			"matches": ["*://*.feedly.com/*"],
			"js": ["js/keypress.js"]
		}
	],
	"background": { "scripts": ["js/background.js"] },
	"options_page": "options.html",
	"permissions": [
		"http://www.feedly.com/*",
		"http://cloud.feedly.com/*",
		"https://cloud.feedly.com/*",
	    "storage"
	],
	"minimum_chrome_version": "21",
	"icons": { "128": "images/icon_128.png" }
}
